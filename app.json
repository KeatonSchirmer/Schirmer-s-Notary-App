{
  "expo": {
    "name": "Schirmers Notary",
    "slug": "frontend",
    "projectId": "140a2bc5-9fae-4a1b-91d1-04b0285a72fb",
    "version": "1.2.0",
    "orientation": "portrait",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "1.2.6",
      "supportsTablet": true,
      "usesPushNotifications": true,
      "icon": "./assets/images/SchirmersNotary.png",
      "bundleIdentifier": "com.keatonschirmer.frontend",
      "aps-environment": "production",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app uses your location to track mileage for your trips.",
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserTrackingUsageDescription": "This identifier will be used to send you notifications.",
        "UIRequiresPersistentWiFi": false
      }
    },
    "notification": {
      "icon": "./assets/images/SchirmersNotary.png",
      "iosDisplayInForeground": true
    },
    "android": {
      "adaptiveIcon": {
        "version": 1,
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SchirmersNotary.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SchirmersNotary.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d2632826-82a5-4678-b911-3ccdcf62864b"
      }
    },
    "sdkVersion": "54.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d2632826-82a5-4678-b911-3ccdcf62864b"
    }
  }
}
